{
"name": "name_of_app",
"version": "0.1.0",
"scripts": {
	"release": "build"
},
"repository": "https://github.com/user_name/repo_name",
"devDependencies": {
	"electron-builder": "10.5.0"
},
"dependencies": {
	"electron-auto-updater": "0.6.2"
},
"build":{
	"appId": "electron-builder",
	"productName": "app description",
	"files":[
		"app/**/*",
		"node_modules/**/*",
		"package.json"
	],
	"linux":{
		"target":[
			"deb"
		]
	},
	"publish":{
		"provider": "github"
	},
	"win":{
		"publish":["github"],
		"remoteReleases": "https://github.com/rrhythmsharma/electron-travis-ci"
	}
}
}
