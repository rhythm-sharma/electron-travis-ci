{
	"name": "electron-quick-start",
	"version": "1.0.0",
	"description": "A minimal Electron application",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"postinstall": "install-app-deps",
		"dist": "npm run build && build --publish onTagOrDraft"
	},
	"repository": "https://github.com/rrhythmsharma/electron-travis-ci",
	"build": {
		"appId": "electron-travis-ci",
		"category": "test",
		"files": [
		  "dist/",
		  "node_modules/",
		  "static/",
		  "main.js",
		  "index.html",
		  "package.json"
		],
		"dmg": {
		  "contents": [
			{
			  "x": 130,
			  "y": 220
			},
			{
			  "x": 410,
			  "y": 220,
			  "type": "link",
			  "path": "/Applications"
			}
		  ]
		},
		"win": {
		  "target": "nsis"
		},
		"linux": {
		  "target": [
			"AppImage",
			"deb"
		  ]
		}
	  },
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"travis-ci"
	],
	"author": "GitHub",
	"license": "CC0-1.0",
	"devDependencies": {
		"electron": "^6.0.2",
		"electron-builder": "^21.2.0"
	}
}
